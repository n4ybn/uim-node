{
  "name": "uim-node",
  "version": "0.0.1",
  "description": "Monitor health of UIM from node-webkit",
  "main": "index.html",

  "repository": {
    "type": "git",
    "url": "https://Alltimed@bitbucket.org/Alltimed/uim-node.git"
  },
  "author": "Bryan Morrow",
  "license": "MIT",
  "chromium-args": "--enable-threaded-compositing --enable-threaded-gpu-rasterization --in-process-gpu",
  "window": {
    "toolbar": true,
    "width": 1024,
    "height": 800,
    "min_width": 1024,
    "min_height": 600,
    "position": "center",
    "resizable": true
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-node-webkit-builder": "^1.0.2"
  },
  "dependencies": {
    "node-localstorage": "^0.5.0",
    "node-rest-client": "^1.4.8"
  }
}
